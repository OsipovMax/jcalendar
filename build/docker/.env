POSTGRES_HOST=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=service_db
POSTGRES_PORT=5432

POSTGRES_VERSION=postgres:13.7-alpine
REDIS_VERSION=redis:6.2.7

POSTGRES=host=${POSTGRES_HOST} user=${POSTGRES_USER} password=${POSTGRES_PASSWORD} dbname=${POSTGRES_DB} port=${POSTGRES_PORT} sslmode=disable TimeZone=Europe/Moscow
REDIS=redis://redis:6379
KAFKA_BROKERS=kafka:19092
REDIS_WORKERS=redis:6379
